.bg {
    height: 150px;
    background-color: #9bebfa;
}

.head {
    display: flex;
    flex-direction: column;
}
   
.headIcon {
    display: flex;
    justify-content: center;
    margin-top: 50rpx;
}
   
.login {
    display: flex;
    color: #000;
    font-size: 15px;
    margin-top: 15rpx;
    justify-content: center;
}
   
.button {
    margin: 10px;
    font-size: 14px;
}
   
.head image {
    border-radius: 50%;
}
   
.hr {
    width: 100%;
    height: 15px;
    background-color: #f4f5f6;
}
   
.item {
    display: flex;
    flex-direction: row;
}
   
.title {
    padding-top: 15px;
    padding-bottom: 15px;
    padding-left: 15px;
    font-size: 15px;
}
   
.detail2 {
    font-size: 15px;
    position: absolute;
    right: 10px;
    height: 50px;
    line-height: 50px;
    color: #888;
}
   
