<view class="container" hover-class="none" hover-stop-propagation="false">
    <view class="top" hover-class="none" hover-stop-propagation="false">
        <image class="" src="../../image/历史.png" style="width: 50px;height: 50px;"></image>
        \n历史文档
    </view>
    <view class="hr"></view>
    <view class="content" hover-class="none" hover-stop-propagation="false">
        <view wx:if="{{history.length==0}}" class="none" hover-class="none" hover-stop-propagation="false">
            还没有历史记录...
        </view>
        <view bind:tap="export" class="item" wx:for="{{history}}" wx:for-item="item" wx:key="index" data-index="{{index}}">
            <image class="suolue" src="../../image/hisicon.png" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload=""></image>
            <view class="mid" hover-class="none" hover-stop-propagation="false">
                <view class="title" hover-class="none" hover-stop-propagation="false">
                    {{item.title}}
                </view>
                <view class="time" hover-class="none" hover-stop-propagation="false">
                    {{item.time}}
                </view>
            </view>
        </view>
    </view>
</view>